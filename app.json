{
  "cli": {
    "version": ">= 16.14.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "default"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk",
        "image": "default"
      },
      "distribution": "internal"
    },
    "production": {
      "android": {
        "buildType": "app-bundle",
        "image": "default"
      },
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  },
  "expo": {
    "name": "WaterFie",                        
    "slug": "appagua",                         
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logoUdc.png",               
    "splash": {
      "image": "./assets/logoUdc.png",          
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "android": {
      "package": "com.mserna.appagua"
    },
    "extra": {
      "eas": {
        "projectId": "7b49b8c6-0bfa-492e-8178-bc06e2ea7801"
      }
    }
  },
